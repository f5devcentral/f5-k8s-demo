            "dns_irule": {
                "class": "iRule",
                "iRule": "\nwhen DNS_REQUEST {\n    #log local0. \"\\[DNS::question name\\]: [DNS::question name], \\[DNS::question class\\]: [DNS::question class], \\[DNS::question type\\]: [DNS::question type]\"\n    set ttl 300\n    if { [string tolower [DNS::question name]] starts_with \"_etcd-server-ssl._tcp.dc1.example.com\" } then {\n        if { [DNS::question type] equals \"SRV\" } then {\n            DNS::answer insert \"_etcd-server-ssl._tcp.dc1.example.com. $ttl IN [DNS::question type] 1 0 2380 etcd-0.dc1.example.com\"\n            DNS::answer insert \"_etcd-server-ssl._tcp.dc1.example.com. $ttl IN [DNS::question type] 1 0 2380 etcd-1.dc1.example.com\"\n            DNS::answer insert \"_etcd-server-ssl._tcp.dc1.example.com. $ttl IN [DNS::question type] 1 0 2380 etcd-2.dc1.example.com\"\n            DNS::return\n        }\n    }\n}  \n"
            },
